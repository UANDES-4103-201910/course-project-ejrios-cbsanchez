<p id="notice"><%= notice %></p>

<h1>Location Admins</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Adminstrator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_admins.each do |location_admin| %>
      <tr>
        <td><%= location_admin.address %></td>
        <td><%= location_admin.latitude %></td>
        <td><%= location_admin.longitude %></td>
        <td><%= location_admin.adminstrator %></td>
        <td><%= link_to 'Show', location_admin %></td>
        <td><%= link_to 'Edit', edit_location_admin_path(location_admin) %></td>
        <td><%= link_to 'Destroy', location_admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location Admin', new_location_admin_path %>
