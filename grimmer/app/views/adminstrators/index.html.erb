<p id="notice"><%= notice %></p>

<h1>Adminstrators</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Password</th>
      <th>Nickname</th>
      <th>Email</th>
      <th>Country</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adminstrators.each do |adminstrator| %>
      <tr>
        <td><%= adminstrator.name %></td>
        <td><%= adminstrator.password %></td>
        <td><%= adminstrator.nickname %></td>
        <td><%= adminstrator.email %></td>
        <td><%= adminstrator.country %></td>
        <td><%= adminstrator.city %></td>
        <td><%= link_to 'Show', adminstrator %></td>
        <td><%= link_to 'Edit', edit_adminstrator_path(adminstrator) %></td>
        <td><%= link_to 'Destroy', adminstrator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Adminstrator', new_adminstrator_path %>
